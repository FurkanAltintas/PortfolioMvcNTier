@model Portfolio.ENTITY.Concrete.Degree

@{
    ViewBag.Title = "Edit";
    Layout = "~/Areas/Dashboard/Views/Shared/DashboardLayout.cshtml";
}

@using (Html.BeginForm("Edit", "Degree", FormMethod.Post))
{
    <div class="form-group">
        @Html.TextBoxFor(x => x.Name, new { @class = "form-control", placeholder = "Ünvan" })
        @Html.ValidationMessageFor(x => x.Name,"", new { @class = "text-danger" })
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-outline-danger">Edit Degree</button>
    </div>
}

